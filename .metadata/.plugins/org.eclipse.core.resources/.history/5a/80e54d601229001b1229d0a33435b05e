package CoreJavaTraining;

public class loop2demo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int k=1;
		for (int i=0; i<4; i++)
		{
			for (int j=1; j<=1; j++)
			{
				System.out.print(k);
				System.out.print("\t"); //gives space
				k++;
				
			}
			System.out.println("");
			
		}

	}

}
